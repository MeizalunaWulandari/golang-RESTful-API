{
	"openapi": "3.0.3",
	"info": "Category RESTful API",
	"description": "Category Spec RESTful API",
	"version": "1.0.0"
},
"servers":[
	{
		"url": "http://localhost:8000/api",
	}
],
"paths":{
	"/categories":{
		"get":{
			"description": "List all categories",
			"response":{
				"200":{
					"description": "Success get all categories",
					"content":{
						"application/json":{
							"schema":{
								"type":"object",
								"properties":{
									"code":{
										"type": "number"
									},
									"status":{
										"type":"string"
									},
									"data":{
										"type":"array",
										"items":{
											"type":"object",
											"properties":{
												"id": {
													"type": "number"
												},
												"name":{
													"type": "string"
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}
}